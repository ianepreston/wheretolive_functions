{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "timerTrigger",
      "direction": "in",
      "name": "timer",
      "schedule": "0 0 6 * * *"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "rfasterqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
